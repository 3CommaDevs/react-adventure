{
  "name": "react-redux-webpack-starter",
  "version": "1.0.2",
  "description": "React Redux Webpack 3 Stater",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --profile --colors",
    "start": "webpack-dev-server --progress --profile --colors",
    "start:dev": " webpack-dashboard -- webpack-dev-server",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "webpack",
    "sass"
  ],
  "author": "Marcos Gon√ßalves <https://github.com/themgoncalves/>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "popper.js": "^1.12.9",
    "react": "^16.2.0",
    "react-dom": "^16.0.0",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "tether": "^1.4.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.0.1",
    "copy-webpack-plugin": "^4.4.2",
    "css-loader": "^0.28.7",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.5",
    "html-webpack-include-assets-plugin": "^1.0.2",
    "html-webpack-plugin": "webpack-contrib/html-webpack-plugin",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "loglevel": "^1.5.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "scss": "^0.2.4",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^4.0.1",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.9",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-setup-env.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|less|css)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/app/lib/**",
      "!src/scss/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "html"
    ]
  }
}
