{
  "name": "react-redux-async-starter-kit",
  "version": "2.0.1",
  "description": "React Adventure",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production  --progress --profile --colors",
    "start": "webpack-dev-server --mode development --progress --profile --colors",
    "start:dev": " webpack-dashboard --mode development -- webpack-dev-server",
    "start:silence": "webpack-dev-server --mode development",
    "lint": "eslint --ext js --ext jsx source || exit 0",
    "lint:fix": "eslint --ext js --ext jsx source --fix|| exit 0",
    "cypress": "cypress run",
    "cypress:open": "cypress open",
    "cypress:record": "cypress run --record",
    "cypress:chrome": "cypress run --browser chrome",
    "cypress:headed": "cypress run --headed",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "run-p --race check:server cypress",
    "test:e2e:open": "run-p --race check:server cypress:open",
    "test:e2e:record": "run-p --race check:server cypress:record",
    "test:e2e:chrome": "run-p --race check:server cypress:chrome",
    "test:e2e:headed": "run-p --race check:server cypress:headed",
    "check:server": "sh ./scripts/server-check.sh localhost:8080"
  },
  "keywords": [
    "react",
    "webpack",
    "sass"
  ],
  "author": "Marcos Gon√ßalves <https://github.com/themgoncalves/>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.1.1",
    "color": "^3.0.0",
    "history": "^4.7.2",
    "lodash": "^4.17.10",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "cypress": "^2.1.0",
    "dotenv": "^5.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.0.0-rc.4",
    "identity-obj-proxy": "^3.0.0",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.3",
    "jest-styled-components": "^5.0.1",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^4.1.2",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.11.1",
    "sass-extract": "^2.1.0",
    "sass-extract-js": "^0.3.0",
    "sass-extract-loader": "^1.1.0",
    "sass-loader": "^7.0.1",
    "scss": "^0.2.4",
    "style-loader": "^0.21.0",
    "svgr": "^1.9.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.1.0",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-subresource-integrity": "^1.1.0-rc.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p --race lint",
      "pre-push": "run-p --race test"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/configuration/jest/setup.env.js",
      "<rootDir>/configuration/jest/setup.styled-components.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/configuration/jest/setup.framework.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleDirectories": [
      "node_modules",
      "source"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|less|css)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/configuration/jest/__mocks__/file.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "source/app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!cypress/**",
      "!configuration/**",
      "!source/app/lib/**",
      "!source/scss/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "html"
    ]
  }
}
