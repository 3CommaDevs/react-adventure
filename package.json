{
  "name": "react-redux-async-starter-kit",
  "version": "1.0.2",
  "description": "React Redux Async Component Starter Kit",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --mode production  --progress --profile --colors",
    "start": "webpack-dev-server --mode development --progress --profile --colors",
    "start:dev": " webpack-dashboard --mode development -- webpack-dev-server",
    "lint": "eslint --ext js --ext jsx src || exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "react",
    "webpack",
    "sass"
  ],
  "author": "Marcos Gon√ßalves <https://github.com/themgoncalves/>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^4.0.0",
    "history": "^4.7.2",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "popper.js": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.3.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "tether": "^1.4.4"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expose-loader": "^0.7.5",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-webpack-include-assets-plugin": "^1.0.4",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "loglevel": "^1.6.1",
    "node-sass": "^4.7.2",
    "postcss-loader": "^2.1.4",
    "react-hot-loader": "^4.0.1",
    "react-test-renderer": "^16.3.2",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^7.0.1",
    "scss": "^0.2.4",
    "style-loader": "^0.20.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cleanup-plugin": "^0.5.1",
    "webpack-cli": "^2.0.14",
    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^3.1.3",
    "webpack-subresource-integrity": "^1.1.0-rc.4"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest-setup-env.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(scss|less|css)$": "identity-obj-proxy",
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/file-mock.js"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/app/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!src/app/lib/**",
      "!src/scss/**",
      "!**/coverage/**"
    ],
    "coverageReporters": [
      "json",
      "lcov",
      "text-summary",
      "html"
    ]
  }
}
